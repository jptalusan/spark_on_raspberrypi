{
 "display_name": "Pyspark Python 3",
 "language": "python3",
 "argv": [
  "/usr/bin/python3",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "SPARK_HOME": "/opt/spark",
  "PYTHONPATH": "/opt/spark/python:/opt/spark/lib/py4j-0.10.4-src.zip",
  "PYTHONSTARTUP": "/opt/spark/python/pyspark/shell.py",
  "PYSPARK_PYTHON": "/usr/bin/python3",
  "PYSPARK_SUBMIT_ARGS": "--master spark://node1:7077 pyspark-shell --no-browser"
 }
}
